{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/style.css", "dest": "/style.css" },
    { "src": "/new_note.html", "dest": "/new_note.html" },
    { "src": "/read_note.html", "dest": "/read_note.html" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
} 